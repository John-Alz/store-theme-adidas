  // //   ],
  // //   "props": {
  // //     "blockClass": "icon__mobile--profile"
  // //   }
  // // },
  "image#home__user--search": {
  "title" : "header user section search logo",
  "props" : {
    "src" : "assets/img/header/search.svg",
    "blockClass" : "home__search--logo",
    "maxWidth" : 20,
    "maxHeight": 18
  }
  },
  "drawer#search": {
    "blocks": [
        "drawer-trigger#mobile__icon--search"
    ],
    "props": {
      "slideDirection": "rightToLeft"
    },
    "children": [
        "search-bar"
    ],
    "title": "Drawer search"
},
"drawer-trigger#mobile__icon--search": {
    "children": [
      "icon#home__user--search"
    ],
    "title": "Icon search Drawer on mobile"
},
  "modal-trigger#example": {
    "title": "header mobile modal tigger",
    "children": [
      "icon#home__user--login" ,
      "modal-layout#example"
    ]
  },
  "modal-layout#example": {
    "title": "header mobile modal layout",
    "children": [
      "login-content#modal__login"
    ]
  },
  "login-content#modal__login" : {
    "title" : "header mobile modal login-content",
    "props" : {
      "defaultOption" : 0
    }
  },

  "flex-layout.row#header__mobile--section-one": {
    "title": "header mobile section one drawer and favorites",
    "children": [
      "drawer",
      "image#Header__user--favorites"
    ],
    "props": {
      "blockClass": "header__mobile--section-one"
    }
  },

  "drawer": {
    "title": "Mega menu mobile",
    "children": [
      "flex-layout.col#mobile__menu--container"
    ],
    "blocks": ["drawer-header#my-drawer"]
  },
  "mega-menu#mobile": {
    "title": "header mobile section mega menu",
    "props": {
      "title": " ",
      "defaultDepartmentActive": "...",
      "orientation": "vertical",
      "openOnly": "vertical"
    }
  },

  "flex-layout.col#mobile__menu--container": {
    "title": "mobile menu container",
    "children": [
      "mega-menu#mobile",
      "flex-layout.col#mobile__menu--items"
    ]
  },

  "flex-layout.col#mobile__menu--items": {
    "title": "mobile menu items",
    "children": [
      "rich-text#mobile__menu--one",
      "rich-text#mobile__menu--two",
      "rich-text#mobile__menu--three",
      "rich-text#mobile__menu--four",
      "rich-text#mobile__menu--five",
      "rich-text#mobile__menu--six"
    ],
    "props": {
      "blockClass": "mobile__menu--items"
    }
  },

  "rich-text#mobile__menu--one": {
    "title": "section muenu text",
    "props": {
      "text": "Mi Cuenta",
      "blockClass": "mobile__menu"
    }
  },

  "rich-text#mobile__menu--two": {
    "title": "section muenu text",
    "props": {
      "text": "Buscador de Tiendas",
      "blockClass": "mobile__menu"
    }
  },

  "rich-text#mobile__menu--three": {
    "title": "section muenu text",
    "props": {
      "text": "Ayuda",
      "blockClass": "mobile__menu"
    }
  },

  "rich-text#mobile__menu--four": {
    "title": "section muenu text",
    "props": {
      "text": "Solicitar mi Devoluici√≥n",
      "blockClass": "mobile__menu"
    }
  },

  "rich-text#mobile__menu--five": {
    "title": "section muenu text",
    "props": {
      "text": "Seguimineto de Pedidos",
      "blockClass": "mobile__menu"
    }
  },

  "rich-text#mobile__menu--six": {
    "title": "section muenu text",
    "props": {
      "text": "Adidas APP",
      "blockClass": "mobile__menu"
    }
  },

  "drawer-header#my-drawer": {
    "title": "header mobile section drawer header",
    "children": ["flex-layout.row#drawer-header"]
  },
  "flex-layout.row#drawer-header": {
    "title": "header mobile section drawer content",
    "children": [
      "flex-layout.col#mega-menu-go-back-btn",
      // "flex-layout.col#logo-desktop",
      "flex-layout.col#drawer-close-button"
    ],
    "props": {
      "blockClass": "drawer-header",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "preserveLayoutOnMobile": true,
      "fullWidth": true
    }
  },
  "flex-layout.col#drawer-close-button": {
    "title": "header mobile section mega menu colse button",
    "children": ["drawer-close-button"],
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "right"
    }
  },
  "flex-layout.col#mega-menu-go-back-btn": {
    "title": "header mobile section mega menu back button",
    "children": ["mega-menu-go-back-btn"],
    "props": {
      "verticalAlign": "middle",
      "horizontalAlign": "left",
      "paddingLeft": 4
    }
  },

  //

  "modal-trigger#mobile__profile--tigger": {
    "title": "header mobile section modal trigger",
    "children": [
      "image#home__user--login",
      "modal-layout#example"
    ],
    "props": {
      "blockClass": "mobile__profile--tigger"
    }
  },
  "modal-layout#mobile__profile--modal": {
    "title": "header mobile section modal layout",
    "children": [
      "login-content#modal__login"
    ],
    "props": {
      "blockClass": "mobile__profile--modal"
    }
  },
  "login-content#mobile__modal--content" : {
    "title" : "header mobile modal login-content",
    "props" : {
      "defaultOption" : 0,
      "blockClass": "mobile__modal--content"
    }
}


