  //   ],
  //   "props": {
  //     "blockClass": "plp__filters--modal"
  //   }
  // },
  "filter-navigator.v3": {
    "title": "filter navigator",
    "props":{
      "blockClass": "desktop__filter--navigator"
    }
  }
}
