{
  "flex-layout.row#pdp__shelf": {
    "title": "home shelf products",
    "children": [
      "list-context.product-list#pdp"
    ],
    "props": {
      "blockClass": "pdp__shelf"
    }
  },

  "list-context.product-list#pdp": {
    "title": "home products list",
        "blocks": ["product-summary.shelf#home__shelf--products"],
        "children": [
          "slider-layout#pdp__slider--products"
        ]
    },

    "slider-layout#pdp__slider--products": {
      "title": "pdp slider shelf products",
      "props": {
        "itemsPerPage": {
          "desktop": 6,
          "tablet": 6,
          "phone": 6
        },
        "infinite": true,
        "showNavigationArrows": "desktopOnly",
        "showPaginationDots": "never",
        "blockClass": [
          "carousel_home",
          "carousel_pdp",
          "carousel_pdp--two"
      ]
      }
    }

