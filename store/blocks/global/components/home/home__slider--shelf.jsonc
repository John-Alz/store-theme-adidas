    //   "props": {
    //     "blockClass": "home__tab1--title",
    //     "text": "PESTAÃ‘A UNO"
    //   }
    // },

    "product-summary-image#product__image": {
      "title": "home product summary image",
      "props": {
        "blockClass": "product__image"
      }
    },
    "product-summary-name#product__name": {
      "title": "home product summary name",
      "props": {
        "blockClass":"product__name"
      }
    },
    // "product-rating-inline#product__rating": {
    //   "props": {
    //     "blockClass":"product__rating"
    //   }
    // },
    "product-summary-space#product__space": {
      "title": "home product summary space",
      "props": {
        "blockClass":"product__space"
      }
    },
    "product-summary-price#product__price":{
      "title": "home product summary price",
      "props": {
        "blockClass":"product__price"
      }
    },
    "add-to-cart-button#product__button": {
      "title": "home product summary add to cart button",
      "props": {
        "blockClass":"product__button"
      }
    }


